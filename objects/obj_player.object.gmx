            <string>//init controls
key_up = -keyboard_check(vk_up);
key_down = keyboard_check(vk_down);
key_left = -keyboard_check(vk_left);
key_right = keyboard_check(vk_right);
key_slingshot = keyboard_check_pressed(ord("Z"));

//determine direction, speed, and facing
h_move = key_left + key_right;
v_move = key_up + key_down;

h_spd = h_move * move_speed;
v_spd = v_move * move_speed;

