    {
        case true:
        facing = p_right;
        break;
        case -true:
        facing = p_left;
        break;
    }
switch (v_move)
    {
        case true:
        facing = p_down;
        break;
        case -true:
        facing = p_up;
        break;
    }
        

//init collision with walls
if place_meeting(x + h_spd, y, obj_wall)
    {
        h_spd = 0;
    }
if place_meeting(x, y + v_spd, obj_wall)
    {
        v_spd = 0;
    }
//update location on the board
x += h_spd;
y += v_spd;

//shooting rocks with a slingshot
if key_slingshot = true
    {
        instance_create(x + 16, y + 10, obj_rock);
    }
 
if health &lt;= 0
    {
        //update once we know how we are handling player death
        game_end();
    }       
</string>
